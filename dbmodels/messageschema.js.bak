var mongodb = require('./mongodb');
var Schema = mongodb.mongoose.Schema;
var MessageSchema = new Schema({
name : String,
alias : [String],
title : String,
age:Number,
create_date : { type: Date, default: Date.now},
images :{
coverSmall:String,
coverBig:String,
},
source :String,
link:String,
lastmessage:string,
lastLocation:string
/**/
});


exports.MessageSchema = MessageSchema;